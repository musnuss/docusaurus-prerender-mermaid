  },
  "peerDependencies": {
    "@docusaurus/core": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "docusaurus",
    "docusaurus-plugin",
    "mermaid",
    "static",
    "prerender",
    "svg"
